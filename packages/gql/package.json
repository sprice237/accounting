{
  "name": "@sprice237/accounting-gql",
  "version": "0.1.0",
  "author": "Sean Price <sean@seanprice.io>",
  "exports": {
    ".": "./dist/index.js",
    "./schema.graphql": "./dist/__autogenerated__/schema.graphql"
  },
  "main": "dist/index.js",
  "files": [
    "dist",
    "documents",
    "schema"
  ],
  "scripts": {
    "build": "run build:gql && ttsc && cp ./src/__autogenerated__/schema.graphql ./dist/__autogenerated__",
    "build:gql": "graphql-codegen && eslint --ext=tsx src/__autogenerated__/types.tsx --fix",
    "clean": "rimraf dist src/__autogenerated__",
    "lint": "eslint --ext=ts,js,tsx,jsx src"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@apollo/client": "3.5.6",
    "@graphql-codegen/add": "3.1.0",
    "@graphql-tools/graphql-file-loader": "7.3.3",
    "@graphql-tools/load": "7.5.0",
    "@graphql-tools/schema": "7.1.5",
    "apollo-link-scalars": "3.0.0",
    "apollo-upload-client": "16.0.0",
    "big.js": "6.1.1",
    "graphql": "15.7.2",
    "graphql-config": "4.0.1",
    "graphql-tag": "2.12.5",
    "graphql-upload": "12.0.0",
    "react": "17.0.2",
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.3.0",
    "@graphql-codegen/introspection": "2.1.0",
    "@graphql-codegen/schema-ast": "2.4.0",
    "@graphql-codegen/typescript": "2.4.1",
    "@graphql-codegen/typescript-operations": "2.2.1",
    "@graphql-codegen/typescript-react-apollo": "3.2.2",
    "@graphql-codegen/typescript-resolvers": "2.4.2",
    "@sprice237/accounting-eslint-config": "workspace:*",
    "@types/apollo-upload-client": "14.1.0",
    "@types/big.js": "6.1.2",
    "@types/graphql-upload": "8.0.7",
    "@types/node": "16.7.1",
    "@types/react": "17.0.19",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-graphql": "4.0.0",
    "eslint-plugin-prettier": "4.0.0",
    "prettier": "2.3.2",
    "rimraf": "3.0.2",
    "ts-node": "10.2.0",
    "ttypescript": "1.5.12",
    "typescript": "4.3.5",
    "typescript-transform-paths": "3.3.1"
  }
